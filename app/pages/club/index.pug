extends ../../templates/club/layout-default
include ../../templates/club/header
include ../../templates/club/index
include ../../templates/club/breadcrumbs
include ../../templates/club/banner-slider
include ../../templates/club/personal-area-buttons
include ../../templates/club/scripts
include ../../templates/club/button
include ../../templates/club/heading
include ../../templates/club/program
include ../../templates/club/cards
include ../../templates/club/partners-slider
include ../../templates/club/footer

block head
    - var pageTitle = 'О Клубе'

block content
    +header()
    +index()
      +e.breadcrumbs
        +breadcrumbs([{ path: '#', title: 'Главная' }, { path: '#', title: 'О клубе' }])
      +e.banner-slider
        +banner-slider()
      +e.personal-area-buttons
        +personal-area-buttons()
      +e.program
        +program()
      +e.cards
        +cards()
      +e.partners-slider
        +partners-slider()
      +e.footer
        +footer()
    +scripts(['index.js'])
